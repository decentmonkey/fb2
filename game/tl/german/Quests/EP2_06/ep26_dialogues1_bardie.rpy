    m "Was machst du?!"

# game/Quests/EP2_06/ep26_dialogues1_bardie.rpy:152
translate german ep26_dialogues1_bardie1_145f141f:

    # bardie "Я дрочу на него, я же говорил тебе."
    bardie "Wichsen, habe ich doch schon erwähnt."

# game/Quests/EP2_06/ep26_dialogues1_bardie.rpy:155
